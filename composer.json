{
    "name": "mvaliolahi/smart-hash",
    "description": "Laravel Smart Hash Model id and request.",
    "authors": [
        {
            "name": "Meysam Valiolahi",
            "email": "mvaliolahi@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
